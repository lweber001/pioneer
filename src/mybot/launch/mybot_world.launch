<?xml version="1.0" encoding="UTF-8"?>
<launch>

<!--                                                             -->
<!--                       Gmapping                              -->


  <arg name="world" default="empty"/> 
  <arg name="paused" default="false"/>
  <arg name="use_sim_time" default="true"/>
  <arg name="gui" default="true"/>
  <arg name="headless" default="false"/>
  <arg name="debug" default="false"/>
  
  <include file="$(find p2os_urdf)/launch/pioneer3at.gazebo.launch">
    <!--arg name="world_name" value="$(find mybot_gazebo)/worlds/mybot.world"/-->

  <!-- Show in Rviz   -->
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find mybot)/rviz/mapping.rviz"/>
  <!--node name="rviz" pkg="rviz" type="rviz" args="-d $(find mybot_description)/launch/myrobot.rviz"/-->

  <master auto="start"/>
  <param name="/use_sim_time" value="true"/>
  <!--- Run gmapping -->
  <node pkg="gmapping" name="slam_gmapping" type="slam_gmapping" output="screen">
    <param name="delta" value="0.01"/>
    <param name="xmin" value="-20"/>
    <param name="xmax" value="20"/>
    <param name="ymin" value="-20"/>
    <param name="ymax" value="20"/>
    <remap from="scan" to="mybot/laser/scan"/>
    <param name="base_frame" value="chassis" />

    <param name="linearUpdate" value="0.5"/>
    <param name="angularUpdate" value="0.436"/>
    <param name="temporalUpdate" value="-1.0"/>
    <param name="resampleThreshold" value="0.5"/>
    <param name="particles" value="80"/>

  </node>

<!--                                                             -->
<!--                       Octomap                               -->

  <!--include file="$(find mybot)/launch/turtlebot_rtabmap.launch" /-->

  <!--include file="$(find turtlebot_gazebo)/launch/turtlebot_world.launch" /-->

  <!--node name="octomap_server" pkg="octomap_server" type="octomap_server_node">
    <remap from="cloud_in" to="/rtabmap/cloud_map" />
    <param name="frame_id" type="string" value="map" />
    <param name="latch" value="false" />
    <param name="resolution" value="0.05" />
  </node-->

  <!--node name="rviz" pkg="rviz" type="rviz" args="-d $(find turtlebot_octomap)/config/octomap_test.rviz" /-->

  <!--node pkg="tf" type="static_transform_publisher" name="map_world_broadcaster" args="0 0 0 0 0 0 /odom /base_footprint  100" /-->

</launch>
